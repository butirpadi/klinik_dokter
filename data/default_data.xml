<odoo>
    <data noupdate="1">

        <record id="sequence_pasien" model="ir.sequence">
            <field name="name">Pasien sequence</field>
            <field name="code">pasien.register.sequence</field>
            <field name="prefix">RM%(year)s</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>

        <record forcecreate="True" id="default_product_category_poliklinik" model="klinik.setting">
            <field name="name">product_category_poliklinik</field>
            <field name="value"></field>
        </record>

    </data>
</odoo>